#!/bin/bash
#PBS -S /bin/bash
#PBS -l mem=3000MB
#PBS -l nodes=1:ppn=2
#PBS -l walltime=23:50:00

#PBS -m bea
#PBS -M aia638@mun.ca

#  Gaussian job script

cd $PBS_O_WORKDIR
echo "Current working directory is `pwd`"
echo "Running on `hostname`"
echo "Starting run at: `date`"

module load gaussian

# Run g09
g09 < glo_2w_beta_boat2_b3lyp_631Gd_iopt.gjf > glo_2w_beta_boat2_b3lyp_631Gd_iopt.out
